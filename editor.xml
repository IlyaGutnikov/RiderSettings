<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;LOGEVENT)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;HACK)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;HARDCODE)(\W|$)(.*)">
      <option name="FOREGROUND" value="cf10d4" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="cf10d4" />
    </pattern>
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;NOTE)(\W|$)(.*)" />
    <pattern pattern="\bNotImplementedException\b" />
    <filter name="ALL" />
  </component>
</application>
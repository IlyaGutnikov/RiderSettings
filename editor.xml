<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="\bNotImplementedException\b" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;HACK)(\W|$)(.*)">
      <option name="FOREGROUND" value="cd05d4" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="cd05d4" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;NOTE)(\W|$)(.*)">
      <option name="FOREGROUND" value="7ad400" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="7ad400" />
    </pattern>
    <filter name="ALL">
      <pattern index="4" />
      <pattern index="3" />
      <pattern index="1" />
      <pattern index="2" />
      <pattern index="0" />
    </filter>
  </component>
</application>